{
  "name": "@sudobility/di_rn",
  "version": "0.1.1",
  "description": "React Native implementations of dependency injection services for Sudobility",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "react-native": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,md}"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "dependency-injection",
    "di",
    "react-native",
    "web3"
  ],
  "author": "Sudobility Inc",
  "license": "MIT",
  "peerDependencies": {
    "@sudobility/di": "^1.4.23",
    "@sudobility/types": "^1.9.18",
    "react": ">=18.0.0",
    "react-native": ">=0.73.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.2.1",
    "@react-navigation/native": "^6.1.9",
    "@react-native-firebase/analytics": "^18.7.0",
    "@notifee/react-native": "^7.8.0",
    "react-native-config": "^1.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnqh/di_rn.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@sudobility/di": "^1.4.23",
    "@sudobility/types": "^1.9.18",
    "@types/node": "^24.10.1",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
